num1 = input("Number 1:")
num2 = input("Number 2:")
num3 = input("Number 3:")

if num1 > num2 and num1 > num3:
    print("The biggest number is Number 1")
elif num2 > num1 and num2 > num3:
    print("The biggest number is Number 2")
elif num3 > num1 and num3 > num2:
    print("The biggest number is Number 3")
